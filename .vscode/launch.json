{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "GDB Launch (build/app)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/app",
      "args": [],
      "cwd": "${workspaceFolder}",
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "preLaunchTask": "C: build (debug)",
      "stopAtEntry": false,
      "externalConsole": false,
      "setupCommands": [
        { "text": "-enable-pretty-printing", "description": "pretty print", "ignoreFailures": true }
      ]
    }
  ]
}
